		<servlet-name> HomeLinkControllerServlet</servlet-name>
		<url-pattern>/HomeLinkControllerServlet</url-pattern>
	</servlet-mapping> 
	
	<servlet>
		<servlet-name>AddPostServlet</servlet-name>
		<servlet-class>com.logitone.webapp.miniproject.servlet.AddPostServlet</servlet-class>
	</servlet>
